<script>
    let logout = () => {
        axios({
            method: 'get',
            url: 'http://localhost:3000/api/logout',
        }).then(result => {
            window.location.replace(
                `http://${window.location.hostname}:${window.location.port}/auth/login`
            )
        })
    }
</script>

